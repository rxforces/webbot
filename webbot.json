{
  "schedule": "R/2015-11-5T00:00:00Z/PT1H",
  "name": "Example Webbot Test",
  "container": {
    "type": "DOCKER",
    "image": "rxforces/webbot"
  },
  "cpus": "1.0",
  "mem": "512",
  "uris": [
    "https://github.com/rxforces/webbot/raw/master/webbot.py"
  ],
  "command": "cd $MESOS_SANDBOX && ./webbot.py",
  "environmentVariables": [
    {
      "name": "HOST",
      "value": "host.to.test"
    },
    {
      "name": "OAUTH_TOKEN",
      "value": "<REPLACEME>"
    },
    {
      "name": "SSH_KEY",
      "value": "<REPLACEME>"
    }
  ]
}
